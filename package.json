{
  "name": "onebrain-platform",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.15.6",
  "scripts": {
    "dev": "cd frontend && pnpm run dev",
    "build": "npx -y pnpm@8.15.6 --dir frontend run build",
    "start": "cd frontend && pnpm run start",
    "lint": "cd frontend && pnpm run lint",
    "install:frontend": "npx -y pnpm@8.15.6 --dir frontend install",
    "heroku-postbuild": "rm -rf frontend/.next && npx -y pnpm@8.15.6 --dir frontend install --no-frozen-lockfile && npx -y pnpm@8.15.6 --dir frontend run build"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0"
  }
}
